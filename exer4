def modificar1(lista):
    lista = [1,1,1]
lista = [2,2,2]
modificar1(lista)
print(lista) 

def modificar2(lista):
    lista[0]=4
    lista[1]=5
    lista[2]=6
lista = [2,2,2]
modificar2(lista)
print(lista) 

def modificar3(lista):
    lista[:] = [1,1,1]
    lista = [2,2,2]
    modificar3(lista)
    print(lista)


def modificar4(lista):
   a = lista [:]
   a = [1,1,1]
   lista = [2,2,2]
   modificar4(lista)
   print(lista)
